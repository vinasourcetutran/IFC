<?xml version="1.0"?>
<desktopMediaUploader xmlns='urn:dmu-schema'>
  <mediaFactory assembly="TheOutfield.UmbExt.DesktopMediaUploader" namespace="TheOutfield.UmbExt.DesktopMediaUploader" type="UmbracoFileMediaFactory">
    <extensions>
      <!-- <ext>*</ext> below is a wildcard and specifies that this MediaFactory 
      will be used if no other MediaFactory has a matching extension. 
      
      If multible MediaFactories is configured with a <ext>*</ext> the first MediaFactory
      in document order will be use-->
      <ext>*</ext>
    </extensions>
  </mediaFactory>
  <mediaFactory assembly="TheOutfield.UmbExt.DesktopMediaUploader" namespace="TheOutfield.UmbExt.DesktopMediaUploader" type="UmbracoImageMediaFactory">
    <extensions>
      <ext>jpeg</ext>
      <ext>jpg</ext>
      <ext>gif</ext>
      <ext>bmp</ext>
      <ext>png</ext>
      <ext>tiff</ext>
      <ext>tif</ext>
    </extensions>
  </mediaFactory>
</desktopMediaUploader>
